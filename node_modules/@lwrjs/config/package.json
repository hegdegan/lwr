{
    "name": "@lwrjs/config",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "version": "0.9.4",
    "homepage": "https://developer.salesforce.com/docs/platform/lwr/overview",
    "repository": {
        "type": "git",
        "url": "https://github.com/salesforce-experience-platform-emu/lwr.git",
        "directory": "packages/@lwrjs/config"
    },
    "bugs": {
        "url": "https://github.com/salesforce-experience-platform-emu/lwr/issues"
    },
    "type": "module",
    "types": "build/es/index.d.ts",
    "main": "build/cjs/index.cjs",
    "module": "build/es/index.js",
    "exports": {
        ".": {
            "import": "./build/es/index.js",
            "require": "./build/cjs/index.cjs"
        },
        "./modules": {
            "import": "./build/es/modules.js",
            "require": "./build/cjs/modules.cjs"
        },
        "./package": "./package.cjs"
    },
    "files": [
        "build/**/*.js",
        "build/**/*.cjs",
        "build/**/*.d.ts",
        "package.cjs"
    ],
    "dependencies": {
        "@lwrjs/diagnostics": "0.9.4",
        "@lwrjs/shared-utils": "0.9.4",
        "fs-extra": "^11.1.0",
        "jsonc-parser": "^3.0.0"
    },
    "devDependencies": {
        "@lwrjs/types": "0.9.4"
    },
    "peerDependencies": {
        "lwc": "2.x"
    },
    "engines": {
        "node": ">=16.0.0 <20"
    },
