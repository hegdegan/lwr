{
    "name": "@lwrjs/shared-utils",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "version": "0.9.4",
    "homepage": "https://developer.salesforce.com/docs/platform/lwr/overview",
    "repository": {
        "type": "git",
        "url": "https://github.com/salesforce-experience-platform-emu/lwr.git",
        "directory": "packages/@lwrjs/shared-utils"
    },
    "scripts": {
        "build": "tsc -b"
    },
    "bugs": {
        "url": "https://github.com/salesforce-experience-platform-emu/lwr/issues"
    },
    "type": "module",
    "types": "build/es/index.d.ts",
    "main": "build/cjs/index.cjs",
    "module": "build/es/index.js",
    "exports": {
        ".": {
            "import": "./build/es/index.js",
            "require": "./build/cjs/index.cjs"
        },
        "./fs-watch": {
            "import": "./build/es/fs-watch.js",
            "require": "./build/cjs/fs-watch.cjs"
        },
        "./typescript": {
            "import": "./build/es/typescript.js",
            "require": "./build/cjs/typescript.cjs"
        },
        "./compiler": {
            "import": "./build/es/compiler.js",
            "require": "./build/cjs/compiler.cjs"
        }
    },
    "files": [
        "build/**/*.js",
        "build/**/*.cjs",
        "build/**/*.d.ts"
    ],
    "dependencies": {
        "es-module-lexer": "^0.3.18",
        "fast-json-stable-stringify": "^2.1.0",
        "magic-string": "^0.25.7",
        "mime-types": "^2.1.33",
        "ms": "^2.1.3",
        "parse5-sax-parser": "^6.0.1",
        "path-to-regexp": "^6.2.0",
        "resolve": "^1.22.1",
        "slugify": "^1.4.5"
    },
    "peerDependencies": {
        "@locker/compiler": "0.18.14",
        "chokidar": "^3.5.3",
        "esbuild": "^0.9.7",
        "rollup": "~2.45.2"
    },
    "devDependencies": {
        "@lwrjs/diagnostics": "0.9.4",
        "@lwrjs/types": "0.9.4",
        "@types/mime-types": "2.1.1",
        "@types/path-to-regexp": "^1.7.0"
    },
    "engines": {
        "node": ">=16.0.0 <20"
    },
    "gitHead": "2a3b831e4ff9c0e45fe3a178b41286d87ec4c1e0"
}
